{
  "name": "@kaggle/monorepo",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "dotenvx run -- turbo dev",
    "dev:client": "dotenvx run -- turbo dev --filter=\"./apps/client/*\"",
    "dev:server": "dotenvx run -- turbo dev --filter=\"./apps/server/*\"",
    "build": "dotenvx run -- turbo build",
    "start": "dotenvx run -- turbo start",
    "lint": "eslint . --fix",
    "fmt": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.17.0"
  },
  "devDependencies": {
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "1.7.35",
    "@types/node": "20.16.11",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "rimraf": "6.0.1",
    "turbo": "2.1.3",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.9.0",
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
